<?xml version="1.0"?>
<FamilyDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<CompilationFlags>
		<COMMONFLAGS>$$com.sysprogs.pic32.legacy_libc$$ -fno-common</COMMONFLAGS>
		<LDFLAGS>$$com.sysprogs.pic32.no_code_in_dinit$$ $$com.sysprogs.pic32.no_dinit_in_serial_mem$$</LDFLAGS>
	</CompilationFlags>
  <ConfigurableProperties>
	<PropertyGroups>
	  <PropertyGroup>
		<Properties>
          <PropertyEntry xsi:type="Boolean">
            <Name>Enable legacy libc</Name>
            <UniqueID>com.sysprogs.pic32.legacy_libc</UniqueID>
            <ValueForTrue>-legacy-libc</ValueForTrue>
            <DefaultValue>true</DefaultValue>
          </PropertyEntry>		  
          <PropertyEntry xsi:type="Boolean">
            <Name>No code in dinit</Name>
            <UniqueID>com.sysprogs.pic32.no_code_in_dinit</UniqueID>
            <ValueForTrue>-Wl,--no-code-in-dinit</ValueForTrue>
            <DefaultValue>true</DefaultValue>
          </PropertyEntry>		  
          <PropertyEntry xsi:type="Boolean">
            <Name>No dinit in serial memory</Name>
            <UniqueID>com.sysprogs.pic32.no_dinit_in_serial_mem</UniqueID>
            <ValueForTrue>-Wl,--no-dinit-in-serial-mem</ValueForTrue>
            <DefaultValue>true</DefaultValue>
          </PropertyEntry>		  
		</Properties>
		<CollapsedByDefault>false</CollapsedByDefault>
	  </PropertyGroup>
	</PropertyGroups>
  </ConfigurableProperties>
	
	
</FamilyDefinition>
