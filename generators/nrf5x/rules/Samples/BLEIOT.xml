<?xml version="1.0"?>
<CommonSampleConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<BasedOn>BLEConfiguration.xml</BasedOn>
	<RequiredFrameworks>
		<string>com.sysprogs.arm.nordic.nrf5x.libraries.external</string>
		<string>com.sysprogs.arm.nordic.nrf5x.mbedtls</string>
		<string>com.sysprogs.arm.nordic.nrf5x.iot</string>
		<string>com.sysprogs.arm.nordic.nrfx.ble_ipsp</string>
	</RequiredFrameworks>
	<DefaultConfiguration>
		<Entries>
			<KeyValue>
				<Key>com.sysprogs.bspoptions.nrf5x.libraries.crypto</Key>
				<Value>yes</Value>
			</KeyValue>
			<KeyValue>
				<Key>com.sysprogs.bspoptions.nrf5x.libraries.external.nrf_cc310</Key>
				<Value>yes</Value>
			</KeyValue>
			<KeyValue>
				<Key>com.sysprogs.bspoptions.nrf5x.iot.medium</Key>
				<Value>yes</Value>
			</KeyValue>
			<KeyValue>
				<Key>com.sysprogs.bspoptions.nrf5x.iot.context_manager</Key>
				<Value>yes</Value>
			</KeyValue>
			<KeyValue>
				<Key>com.sysprogs.bspoptions.nrf5x.libraries.mem_manager</Key>
				<Value>yes</Value>
			</KeyValue>
			<KeyValue>
				<Key>com.sysprogs.bspoptions.nrf5x.iot.ble_6lowpan</Key>
				<Value>yes</Value>
			</KeyValue>
			<KeyValue>
				<Key>com.sysprogs.bspoptions.nrf5x.iot.iot_timer</Key>
				<Value>yes</Value>
			</KeyValue>
		</Entries>
	</DefaultConfiguration>
</CommonSampleConfiguration>