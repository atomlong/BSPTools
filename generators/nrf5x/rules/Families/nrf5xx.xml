								<UniqueID>com.sysprogs.bspoptions.nrf5x.twi_driver_legacy</UniqueID>
								<DefaultValue>false</DefaultValue>
								<ValueForTrue>yes</ValueForTrue>
							</PropertyEntry>
							<PropertyEntry xsi:type="Boolean">
								<Name>Legacy Clock driver</Name>
								<UniqueID>com.sysprogs.bspoptions.nrf5x.clock_driver_legacy</UniqueID>
								<DefaultValue>false</DefaultValue>
								<ValueForTrue>yes</ValueForTrue>
							</PropertyEntry>
							<PropertyEntry xsi:type="Boolean">
								<Name>Legacy UART driver</Name>
								<UniqueID>com.sysprogs.bspoptions.nrf5x.uart_driver_legacy</UniqueID>
								<DefaultValue>false</DefaultValue>
								<ValueForTrue>yes</ValueForTrue>
							</PropertyEntry>
						</Properties>
						<CollapsedByDefault>false</CollapsedByDefault>
					</PropertyGroup>
				</PropertyGroups>
			</ConfigurableProperties>
