					<AutoIncludeMask>*.h</AutoIncludeMask>
					<SimpleFileConditions>
						<string>legacy\\nrf_drv_twi*: $$com.sysprogs.bspoptions.nrf5x.twi_driver_legacy$$ == yes</string>
						<string>legacy\\nrf_drv_swi*: $$com.sysprogs.bspoptions.nrf5x.swi_driver_legacy$$ == yes</string>
						<string>legacy\\nrf_drv_uart*: $$com.sysprogs.bspoptions.nrf5x.uart_driver_legacy$$ == yes</string>
