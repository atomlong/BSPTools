					<Expression>$$com.sysprogs.bspoptions.nrf5x.softdevice$$</Expression>
					<ExpectedValue>nosoftdev</ExpectedValue>
				</Argument>
			</FlagCondition>
			<Flags>
				<PreprocessorMacros>
					<string>SOFTDEVICE_PRESENT</string>
				</PreprocessorMacros>
			</Flags>
		</ConditionalToolFlags>
		<ConditionalToolFlags>
			<FlagCondition xsi:type="Equals">
				<Expression>$$com.sysprogs.bspoptions.nrf5x.softdevice$$</Expression>
				<ExpectedValue>nosoftdev</ExpectedValue>
			</FlagCondition>
			<Flags>
				<IncludeDirectories>
